{
  "name": "react-seed",
  "version": "0.0.1",
  "private": true,
  "description": "Seed for react projects",
  "scripts": {
    "start": "webpack-dev-server --config ./config/webpack.config.local.js",
    "copy:html": "node tools/copy-html.js",
    "test": "mocha tools/test-setup.js \"app/**/*.js\"",
    "test:watch": "npm run test -- --watch",
    "lint": "esw app",
    "lint:watch": "npm run lint -- --watch",
    "prebuild": "rimraf ./dist",
    "build": "webpack",
    "postbuild": "npm run copy:html"
  },
  "dependencies": {
    "core-js": "2.5.0",
    "flex-grill": "0.0.2",
    "prop-types": "15.5.10",
    "react": "15.6.1",
    "react-dom": "15.6.1"
  },
  "devDependencies": {
    "autoprefixer": "7.1.3",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.1.2",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-register": "6.26.0",
    "chalk": "2.1.0",
    "cheerio": "0.22.0",
    "css-loader": "0.28.5",
    "enzyme": "2.9.1",
    "eslint": "4.5.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-react": "7.3.0",
    "eslint-watch": "3.1.2",
    "expect": "1.20.2",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "jsdom": "8.5.0",
    "mocha": "3.5.0",
    "node-sass": "4.5.3",
    "npm-run-all": "4.1.1",
    "postcss-loader": "2.0.6",
    "progress-bar-webpack-plugin": "1.10.0",
    "react-addons-test-utils": "15.6.0",
    "react-test-renderer": "15.6.1",
    "rimraf": "^2.6.1",
    "sass-loader": "6.0.6",
    "style-loader": "0.18.2",
    "url-loader": "0.5.9",
    "webpack": "3.5.5",
    "webpack-dev-server": "^2.7.1",
    "webpack-merge": "4.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kurtz1993/react-seed"
  }
}